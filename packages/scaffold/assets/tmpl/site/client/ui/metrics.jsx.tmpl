'use strict'

import { RenderingCountMetrics } from '@the-/metrics-presets'
import * as stateful from './stateful'
import * as v from './views'

/** @lends module:~~~~package_name~~~~/client.metrics */
function metrics() {
  // Count renders of react
  {
    const Components = { ...v, ...stateful }
